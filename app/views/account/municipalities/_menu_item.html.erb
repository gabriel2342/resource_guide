<% if can? :read, Municipality.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :municipalities]),
    label: t('municipalities.navigation.label'),
  } do |p| %>
    <% p.content_for :icon do %>
      <i class="<%= t('municipalities.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
